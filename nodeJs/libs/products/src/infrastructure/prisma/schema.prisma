generator client {
  provider = "prisma-client-js"
  output   = "./generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL_PRODUCTS")
}

model Product {
  id         String   @id @default(cuid())
  name       String
  priceCents Int
  createdAt  DateTime @default(now())
  updatedAt  DateTime @updatedAt
}